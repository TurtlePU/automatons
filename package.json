{
  "name": "automatons",
  "version": "0.0.1",
  "description": "cell automatons for fun",
  "private": true,
  "scripts": {
    "clear-serve": "rm -rf dist",
    "serve": "npm run clear-serve && parcel src/index/index.html",
    "clear-build": "rm -rf build",
    "build": "npm run clear-build && parcel build src/index/index.html --out-dir build --log-level 1 --no-source-maps --experimental-scope-hoisting"
  },
  "author": "Turtle P.U.",
  "license": "MIT",
  "dependencies": {
    "pixi.js": "^5.0.2"
  },
  "devDependencies": {
    "@types/pixi.js": "^4.8.7",
    "parcel-bundler": "^1.12.3",
    "typescript": "^3.4.5"
  }
}
